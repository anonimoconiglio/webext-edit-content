{
	"manifest_version": 2,
	"name": "Edit Page Content",
	"version": "1.0",
	"description": "Add an option on the context menu to start or stop editing the content of the page",
	"homepage_url": "https://github.com/anonimoconiglio/webext-edit-content/tree/browser/chrome",
	"icons": {
		"80": "icons/edit80.png"
	},
	"permissions": [
		"activeTab",
		"contextMenus"
	],
	"background": {
		"scripts": ["backgroundScript.js"]
	}
}
